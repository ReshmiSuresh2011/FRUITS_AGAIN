context PurchaseOrder 
{
	entity Items 
	{
		key PURCHASEORDERID : Integer;
		AMOUNT : Decimal(15, 2);
		STATUS : String(20);
		STATUSAGAIN: String(1);
	};
 
     define view ItemView as
	select from Items
	{
		PURCHASEORDERID as "PurchaseOrderItemId",
		AMOUNT          as "Amount",
		STATUS			as "Status"
	};
};